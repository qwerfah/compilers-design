<grammar name="G0">
    <terminalsymbols>
        <term name="{" spell="{"/>
        <term name="}" spell="}"/>
        <term name="(" spell="("/>
        <term name=")" spell=")"/>
        <term name=";" spell=";"/>
        <term name="=" spell="="/>
        <term name="*" spell="*"/>
        <term name="/" spell="/"/>
        <term name="+" spell="+"/>
        <term name="-" spell="-"/>
        <term name="&lt;" spell="&lt;"/>
        <term name="&gt;" spell="&gt;"/>
        <term name="&lt;=" spell="&lt;="/>
        <term name="&gt;=" spell="&gt;="/>
        <term name="&lt;&gt;" spell="&lt;&gt;"/>
        <term name="or" spell="or"/>
        <term name="not" spell="not"/>
        <term name="and" spell="and"/>
        <term name="div" spell="div"/>
        <term name="mod" spell="mod"/>
        <term name="or" spell="or"/>
        <term name="id" spell="id"/>
        <term name="const" spell="const"/>
    </terminalsymbols>
    <nonterminalsymbols>
        <nonterm name="program"/>
        <nonterm name="block"/>
        <nonterm name="operator_list"/>
        <nonterm name="tail"/>
        <nonterm name="operator"/>
        <nonterm name="expr"/>
        <nonterm name="simple_expr"/>
        <nonterm name="term"/>
        <nonterm name="factor"/>
        <nonterm name="sign"/>
        <nonterm name="rel_op"/>
        <nonterm name="sum_op"/>
        <nonterm name="mul_op"/>
        <nonterm name="simple_expr#0"/>
        <nonterm name="term#0"/>
    </nonterminalsymbols>
    <productions>
        <production>
            <lhs name="program"/>
            <rhs>
                <symbol type="nonterm" name="block"/>
            </rhs>
        </production>
        <production>
            <lhs name="block"/>
            <rhs>
                <symbol type="term" name="{"/>
                <symbol type="nonterm" name="operator_list"/>
                <symbol type="term" name="}"/>
            </rhs>
        </production>
        <production>
            <lhs name="operator_list"/>
            <rhs>
                <symbol type="nonterm" name="operator"/>
                <symbol type="nonterm" name="tail"/>
            </rhs>
        </production>
        <production>
            <lhs name="tail"/>
            <rhs>
                <symbol type="term" name=";"/>
                <symbol type="nonterm" name="operator"/>
                <symbol type="nonterm" name="tail"/>
            </rhs>
        </production>
        <production>
            <lhs name="tail"/>
            <rhs/>
        </production>
        <production>
            <lhs name="operator"/>
            <rhs>
                <symbol type="term" name="id"/>
                <symbol type="term" name="="/>
                <symbol type="nonterm" name="expr"/>
            </rhs>
        </production>
        <production>
            <lhs name="expr"/>
            <rhs>
                <symbol type="nonterm" name="simple_expr"/>
            </rhs>
        </production>
        <production>
            <lhs name="expr"/>
            <rhs>
                <symbol type="nonterm" name="simple_expr"/>
                <symbol type="nonterm" name="rel_op"/>
                <symbol type="nonterm" name="simple_expr"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr"/>
            <rhs>
                <symbol type="nonterm" name="term"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr"/>
            <rhs>
                <symbol type="nonterm" name="sign"/>
                <symbol type="nonterm" name="term"/>
            </rhs>
        </production>
        <production>
            <lhs name="term"/>
            <rhs>
                <symbol type="nonterm" name="factor"/>
            </rhs>
        </production>
        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="id"/>
            </rhs>
        </production>
        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="const"/>
            </rhs>
        </production>
        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="("/>
                <symbol type="nonterm" name="simple_expr"/>
                <symbol type="term" name=")"/>
            </rhs>
        </production>
        <production>
            <lhs name="factor"/>
            <rhs>
                <symbol type="term" name="not"/>
                <symbol type="nonterm" name="factor"/>
            </rhs>
        </production>
        <production>
            <lhs name="sign"/>
            <rhs>
                <symbol type="term" name="+"/>
                <symbol type="term" name="-"/>
            </rhs>
        </production>
        <production>
            <lhs name="rel_op"/>
            <rhs>
                <symbol type="term" name="="/>
                <symbol type="term" name="&lt;&gt;"/>
                <symbol type="term" name="&lt;"/>
                <symbol type="term" name="&lt;="/>
                <symbol type="term" name="&gt;"/>
                <symbol type="term" name="&gt;="/>
            </rhs>
        </production>
        <production>
            <lhs name="sum_op"/>
            <rhs>
                <symbol type="term" name="+"/>
                <symbol type="term" name="-"/>
                <symbol type="term" name="or"/>
            </rhs>
        </production>
        <production>
            <lhs name="mul_op"/>
            <rhs>
                <symbol type="term" name="*"/>
                <symbol type="term" name="/"/>
                <symbol type="term" name="div"/>
                <symbol type="term" name="mod"/>
                <symbol type="term" name="and"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr"/>
            <rhs>
                <symbol type="nonterm" name="term"/>
                <symbol type="nonterm" name="simple_expr#0"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr"/>
            <rhs>
                <symbol type="nonterm" name="sign"/>
                <symbol type="nonterm" name="term"/>
                <symbol type="nonterm" name="simple_expr#0"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr#0"/>
            <rhs>
                <symbol type="nonterm" name="sum_op"/>
                <symbol type="nonterm" name="term"/>
            </rhs>
        </production>
        <production>
            <lhs name="simple_expr#0"/>
            <rhs>
                <symbol type="nonterm" name="sum_op"/>
                <symbol type="nonterm" name="term"/>
                <symbol type="nonterm" name="simple_expr#0"/>
            </rhs>
        </production>
        <production>
            <lhs name="term"/>
            <rhs>
                <symbol type="nonterm" name="factor"/>
                <symbol type="nonterm" name="term#0"/>
            </rhs>
        </production>
        <production>
            <lhs name="term#0"/>
            <rhs>
                <symbol type="nonterm" name="mul_op"/>
                <symbol type="nonterm" name="factor"/>
            </rhs>
        </production>
        <production>
            <lhs name="term#0"/>
            <rhs>
                <symbol type="nonterm" name="mul_op"/>
                <symbol type="nonterm" name="factor"/>
                <symbol type="nonterm" name="term#0"/>
            </rhs>
        </production>
    </productions>
    <startsymbol name="program"/>
</grammar>
