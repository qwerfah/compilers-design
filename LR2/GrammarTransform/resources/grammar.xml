<?xml version="1.0" encoding="UTF-8"?>
<grammar name="G0">
    <terminalsymbols>
        <term name="{" spell="{" />
        <term name="}" spell="}" />
        <term name="(" spell="(" />
        <term name=")" spell=")" />
        <term name=";" spell=";" />
        <term name="=" spell="=" />
        <term name="+" spell="+" />
        <term name="-" spell="-" />
        <term name="<" spell="<" />
        <term name=">" spell=">" />
        <term name="<=" spell="<=" />
        <term name=">=" spell=">=" />
        <term name="<>" spell="<>" />
        <term name="or" spell="or" />
        <term name="and" spell="and" />
        <term name="div" spell="div" />
        <term name="mod" spell="mod" />
        <term name="or" spell="or" />
    </terminalsymbols>
    <nonterminalsymbols>
        <nonterm name="program" />
        <nonterm name="block" />
        <nonterm name="operator_list" />
        <nonterm name="tail" />
        <nonterm name="operator" />
        <nonterm name="expr" />
        <nonterm name="simple_expr" />
        <nonterm name="term" />
        <nonterm name="factor" />
        <nonterm name="sign" />
        <nonterm name="rel_op" />
        <nonterm name="sum_op" />
        <nonterm name="mul_op" />
        <nonterm name="id" />
        <nonterm name="const" />
    </nonterminalsymbols>
    <productions>
        <production>
            <lhs name="program" />
            <rhs>
                <symbol type="nonterm" name="block" />
            </rhs>
        </production>
        <production>
            <lhs name="block" />
            <rhs>
                <symbol type="term" name="i" />
                <symbol type="nonterm" name="E" />
                <symbol type="term" name="t" />
                <symbol type="nonterm" name="S" />
                <symbol type="term" name="e" />
                <symbol type="nonterm" name="S" />
            </rhs>
        </production>
        <production>
            <lhs name="S" />
            <rhs>
                <symbol type="term" name="a" />
            </rhs>
        </production>
        <production>
            <lhs name="E" />
            <rhs>
                <symbol type="nonterm" name="b" />
            </rhs>
        </production>
    </productions>
    <startsymbol name="S" />
</grammar>
